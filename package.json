{
  "name": "node-clean-starter",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "nodemon --exec \"ts-node --swc -r tsconfig-paths/register\" src/infra/http/server.ts",
    "doc:generate": "ts-node --swc -r tsconfig-paths/register ./generate-swagger-file",
    "build": "rm -rf ./dist && swc ./src -d ./dist",
    "test": "jest --passWithNoTests",
    "test:w": "jest -- --watch",
    "coverage": "jest -- --coverage",
    "commit": "git-cz",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@swc/cli": "^0.1.61",
    "@swc/core": "^1.3.35",
    "@swc/helpers": "^0.4.14",
    "@swc/jest": "^0.2.24",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.4.2",
    "lint-staged": "^13.1.0",
    "mongodb-memory-server": "^8.11.4",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "prettier-plugin-organize-imports": "^3.2.2",
    "regenerator-runtime": "^0.13.11",
    "resolve-tspaths": "^0.8.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@typegoose/typegoose": "^10.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mongoose": "^6.9.0",
    "reflect-metadata": "^0.1.13",
    "swagger-decorators": "^1.0.0",
    "tsyringe": "^4.7.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  }
}
